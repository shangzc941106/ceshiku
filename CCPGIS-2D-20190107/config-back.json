                "layer": "vec",
                "icon": "tianditumap.png"
            },
            {
                "pid": 10,
                "name": "谷歌影像",
                "type": "google_yx",
                "layer": "img",
                "icon": "tianditumap.png"
            }

        ],
        "operationallayers": [
            { "id": 20, "name": "辅助图层", "type": "group" },
            { "pid": 20, "name": "经纬网", "type": "custom_jww" },
            { "pid": 20, "name": "实时路况", "type": "www_gaode", "layer": "time" },

            {
                "id": 40,
